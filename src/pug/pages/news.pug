extends ../_template

mixin news-list__item(filter, image, title, subtitle, text)
  //begin news-list__item
  .news-list__item.news-list__item--filter(data-filter= filter)
    .news-list__pic
      img(src="assets/img/news/news-" + image + ".png" alt="image")
    h3.news-list__title= title
    p
      span= subtitle
    p= text
    a(href="#").news-list__link читать полный отзыв
  //end news-list__item

block variables
  - var title = 'Наши клиенты'
  - var pageHeaderTitle = 'Наши клиенты'

