extends ../_template

mixin clients-list__item(filter, image, title, subtitle, text)
  //begin clients-list__item
  .clients-list__item(data-filter= filter)
    .clients-list__pic
      img(src="assets/img/clients/clients-" + image + ".png" alt="image")
    h3.clients-list__title= title
    p
      span= subtitle
    p= text
    a(href="#").clients-list__link читать полный отзыв
  //end clients-list__item

block variables
  - var title = 'Наши клиенты'
  - var pageHeaderTitle = 'Наши клиенты'

